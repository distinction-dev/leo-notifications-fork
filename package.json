{
  "name": "serverless-ts-template",
  "version": "0.0.0-development",
  "description": "Serverless Typescript template",
  "scripts": {
    "lint": "eslint . --ext js,ts --cache --fix",
    "prettier": "prettier --list-different './**/*.{js,ts}'",
    "type-check": "tsc --noEmit",
    "package": "sls package",
    "deploy": "sls deploy",
    "gen:spec": "ts-node openapi.ts"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@distinction-dev/lambda-authorizer-utils": "^1.12.1",
    "@middy/core": "^4.2.1",
    "@middy/http-error-handler": "^4.2.1",
    "@middy/http-json-body-parser": "^4.2.1",
    "@middy/validator": "^4.2.1",
    "@sentry/serverless": "^7.34.0",
    "@sentry/tracing": "^7.34.0",
    "dynamoose": "^3.1.0",
    "envalid": "^7.3.1",
    "iam-floyd": "^0.479.0",
    "nanoid": "^4.0.0",
    "serverless-offline": "^11.6.0",
    "serverless-prune-plugin": "^2.0.1",
    "serverless-step-functions": "^3.11.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.110",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "json-schema-to-ts": "^2.6.2",
    "openapi-types": "^12.1.0",
    "prettier": "^2.8.0",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-schema": "^1.16.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "Distinction Dev",
  "license": "MIT"
}
